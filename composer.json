{
    "name": "codedive-io/laravel-code-verification",
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "orchestra/testbench": "^10.6"
    },
    "autoload": {
        "psr-4": {
            "Codedive\\LaravelCodeVerification\\": "src/",
            "Codedive\\LaravelCodeVerification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedive\\LaravelCodeVerification\\CodeVerificationServiceProvider"
            ],
            "aliases": {
                "CodeVerification": "Codedive\\LaravelCodeVerification\\CodeVerification"
            }
        }
    },
    "authors": [
        {
            "name": "foxtrot@codedive.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^12.0"
    }
}
